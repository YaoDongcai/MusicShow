.my-ul {
  padding-left: 30rpx;
  padding-right: 30rpx;
  background-color: white;
}
.my-ul-li {
  height: 108rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.my-ul-li-left {
  width: 56rpx;
  height: 56rpx;
  padding-right: 24rpx;
}
.my-ul-li-left image {
  width: 56rpx;
  height: 56rpx;
}
.my-ul-li-right {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  position: relative;
  font-size: 34rpx;
}
.my-ul-li-right::after {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 200%;
  height: 200%;
  -webkit-transform: scale(0.5);
          transform: scale(0.5);
  -webkit-transform-origin: 0 0;
          transform-origin: 0 0;
  pointer-events: none;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  border: 0 solid #d8d8d8;
  border-bottom-width: 1px;
  border-bottom-color: #D8D8D8;
  top: 30rpx;
  left: 0px;
}
.my-ul-li-right::before {
  content: " ";
  display: inline-block;
  width: 6px;
  height: 6px;
  position: absolute;
  top: 50%;
  right: 2px;
  border-width: 2px 2px 0 0;
  border-color: #B0B0B0;
  border-style: solid;
  -webkit-transform: translateY(-50%) matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
          transform: translateY(-50%) matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
}
.my .my-block {
  height: 120rpx;
  background-color: #f3f3f3;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding-left: 30rpx;
}
.my .my-block .left {
  margin-top: 10rpx;
}
.my .my-block .left image {
  height: 40rpx;
  width: 40rpx;
}
.my .my-block .right {
  margin-top: 10rpx;
  margin-left: 10rpx;
}
.my .my-block .right ._span {
  display: inline-block;
  color: #f20909;
  margin-right: 10rpx;
}
.my .my-header {
  width: 100vw;
  height: 260rpx;
  position: relative;
}
.my .my-header image {
  width: 100vw;
  height: 260rpx;
}
.my .my-header .my-header-content {
  position: absolute;
  top: 0rpx;
  left: 60rpx;
  width: 200rpx;
  margin-top: 80rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.my .my-header .my-header-content .left image {
  height: 100rpx;
  width: 100rpx;
  border-radius: 50%;
}
.my .my-header .my-header-content .right {
  line-height: 100rpx;
  margin-left: 20rpx;
  color: white;
  font-size: 36rpx;
}

